<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:width="30dp"
        android:height="10dp"
        android:top="35dp"
        android:right="49dp"
        android:gravity="center">
        <rotate
            android:fromDegrees="36">
            <shape android:shape="rectangle">
                <solid android:color="#000000"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="70dp"
        android:height="10dp"
        android:left="12dp"
        android:top="4dp"
        android:gravity="center"
        >
        <rotate
            android:fromDegrees="-50">
            <shape android:shape="rectangle">
                <solid android:color="#000000"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="60dp"
        android:height="60dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>

</layer-list>
